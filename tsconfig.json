{
  // https://nuxt.com/docs/guide/concepts/typescript
  // "extends": "./nuxt.tsconfig.json",
  "target": "ES2018",
  "module": "ESNext",
  "moduleResolution": "Node",
  "lib": [
    "ESNext",
    "ESNext.AsyncIterable",
    "DOM",
    "ES2017",
    "dom",
    "ES2015",
  ],
  "esModuleInterop": true,
  "allowJs": true,
  "sourceMap": true,
  "strict": true,
  "noEmit": true,
  "experimentalDecorators": true,
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "types": [
      "@nuxt/types",
      "@nuxtjs/axios",
      "@nuxtjs/auth-next",
      "node"
    ]
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}